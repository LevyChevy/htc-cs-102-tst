<Window x:Class="FinalProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="214*"/>
            <RowDefinition Height="105*"/>
        </Grid.RowDefinitions>
        <Button x:Name="Submit" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="393,20.772,0,0" Click="Submit_Click" Grid.Row="1"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="87" Margin="149,20.772,0,0" Grid.Row="1"/>
        <TextBox x:Name="UsernameBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="149,184,0,0" TextChanged="UsernameBox_TextChanged"/>

    </Grid>
</Window>
